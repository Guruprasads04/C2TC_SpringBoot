<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="Guru" class="com.tns.dependencyInjection.Student">
	 <property name="sid" value="12"/>
	 <property name="sname" value="joseph Vijay"/>
	 <property name="city" value="Puducherry"/>
</bean>

<bean id="Prasad" class="com.tns.dependencyInjection.Student">
	 <property name="sid" value="13"/>
	 <property name="sname" value="arjuin"/>
	 <property name="city" value="Villupuram"/>
</bean>

<bean id="livya" class="com.tns.dependencyInjection.Student1">
	 <constructor-arg name="sid" value="14"/>
	 <constructor-arg name="sname" value="Sakthi H"/>
	 <constructor-arg name="city" value="Chennai"/>
</bean>

<bean id="retz" class="com.tns.dependencyInjection.Student1">
	 <constructor-arg name="sid" value="14"/>
	 <constructor-arg name="sname" value="Hema"/>
